Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

URxvt.geometry: 120x50
URxvt.depth: 32
URxvt.internalBorder: 5
URxvt.background: #111111
URxvt.fading: 10

URxvt.scrollBar: off

URxvt.urgentOnBell: True
URxvt.visualBell: False

URxvt.perl-ext-common: default,selection-to-clipboard,font-size

URxvt.font: xft:Tewi:pixelsize=11
URxvt.font-size.zoomfont: xft:Terminess Powerline:pixelsize=13
URxvt.font-size.step: 2
URxvt.keysym.C-Up:   perl:font-size:increase
URxvt.keysym.C-Down: perl:font-size:decrease
URxvt.keysym.C-Left: perl:font-size:reset

*Menu.background: Black
*Menu.foreground: White

*cursorColor: #F3F3F3
